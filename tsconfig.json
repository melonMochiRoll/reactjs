{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "outDir": "./dist",
    "allowJs": true,
    "moduleResolution": "node",
    "esModuleInterop" : true,
    "allowSyntheticDefaultImports" : true,
    "baseUrl": ".",
    "paths": {
      "Src/*": ["src/*"],
      "Constants/*": ["src/constants/*"],
      "Hooks/*": ["src/hooks/*"],
      "Pages/*": ["src/pages/*"],
      "Styles/*": ["src/styles/*"],
      "Typings/*": ["src/typings/*"]
    },
    "typeRoots": ["@typings"],
  }
}